#include "holberton.h"

/**
 * main - Entry point
 *
 * This program prints a string
 *
 * Return: Always 0 (success)
 */
int main(void)
{
	int count;
	char schoolName[] = "Holberton";

    
	for (count = 0; count < 9; count++)
	{
		_putchar(schoolName[count]);
	}
	
	_putchar('\n');
	
	return (0);
}
